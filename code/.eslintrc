{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["prettier", "simple-import-sort"],
  "rules": {
    "prettier/prettier": "error",
    "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "error",
    "no-console": "warn",
    "no-unused-vars": "warn",
    "'no-constant-binary-expression": "warn"
  }
}
